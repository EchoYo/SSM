<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.echo.erpMapper.InventoryMapper">
    <select id="getByCode" resultType="com.echo.model.Inventory" parameterType="String">
        select cInvCode,cInvAddCode,cInvName from Inventory where cInvCCode = '02' and  cInvAddCode = #{cInvAddCode}
    </select>
</mapper>